extends base

block content
  header


  .calendar-container
    #calendar
  
  .top-posts
    h2 인기 게시물 🔥
    ul 
      each post in top5Contents 
        li 
          small [#{post.prefix}]
          a(href=`/post/${post._id}`) #{post.title}
          small 👍#{post.meta.like}
  
  


  link(rel='stylesheet', href='/fullcalendar/main.css')
  link(rel='stylesheet', href='/style.css')
  script(src='/jquery/dist/jquery-3.7.1.min.js')
  script(src='/fullcalendar/main.js')
  script(src='/javascripts/main.js')

